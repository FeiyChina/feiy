<h1>Edit#new</h1>


<%= simple_form_for(@organization) do |f| %>
  <%= f.input :name %>
  <%= f.input :problem %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :website %>
  <%= f.input :photo, as: :attachinary %>
  <%= f.input :logo, as: :attachinary %>
  <%= f.submit %>
<% end %>
